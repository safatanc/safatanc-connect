{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT \n                b.id, b.name, b.description, b.image_url,\n                b.created_at, b.updated_at, b.deleted_at\n            FROM badges b\n            JOIN user_badges ub ON b.id = ub.badge_id\n            WHERE ub.user_id = $1 \n              AND ub.deleted_at IS NULL\n              AND b.deleted_at IS NULL\n            ORDER BY ub.created_at DESC\n            ",
