  "query": "\n            SELECT \n                id, user_id, token, type as \"token_type\", expires_at, used_at,\n                created_at, updated_at\n            FROM verification_tokens\n            WHERE user_id = $1 AND type = $2 \n            AND used_at IS NULL AND expires_at > NOW()\n            ORDER BY created_at DESC\n            ",
