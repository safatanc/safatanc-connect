  "query": "\n            UPDATE badges\n            SET\n                name = COALESCE($1, name),\n                description = COALESCE($2, description),\n                image_url = COALESCE($3, image_url),\n                updated_at = now()\n            WHERE id = $4 AND deleted_at IS NULL\n            RETURNING \n                id, name, description, image_url,\n                created_at, updated_at, deleted_at\n            ",
